{
  "easycom": {
    "autoscan": true,
    "custom": {
      // uni-ui 规则如下配置
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^U(.*)": "@/components/UnoUI/U$1/U$1.vue"
    }
  },
  "pages": [ // pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页"
      },
      "meta": {
        "ignoreAuth": true
      }
    },
    {
      "path": "pages/alarm/index",
      "style": {
        "navigationBarTitleText": "警告"
      },
      "meta": {
        "ignoreAuth": true
      }
    },
    {
      "path": "pages/about/index",
      "style": {
        "navigationBarTitleText": "我的"
      },
      "meta": {
        "ignoreAuth": true
      }
    },
    {
      "path": "pages/login/index",
      "style": {
        "navigationBarTitleText": "登录"
      },
      "meta": {
        "ignoreAuth": true
      }
    },
    {
      "path": "pages/device/index",
      "style": {
        "navigationBarTitleText": "站点"
      },
      "meta": {
        "ignoreAuth": true
      }
    },
    {
      "path": "pages/log/index",
      "style": {
        "navigationBarTitleText": "日志"
      }
    },
    {
      "path": "pages/notFound/404",
      "style": {
        "navigationBarTitleText": "Not Found"
      },
      "meta": {
        "ignoreAuth": true
      }
    }
  ],
  "subPackages": [
    {
      "root": "demo/example",
      "pages": [
        {
          "path": "fontAwesomeIcon/index",
          "style": {
            "navigationBarTitleText": "FontAwesomeIcon-Demo"
          },
          "meta": {
            "ignoreAuth": true
          }
        },
        {
          "path": "fontAwesomeIcon/check",
          "style": {
            "navigationBarTitleText": "FontAwesomeIcon-Check"
          },
          "meta": {
            "ignoreAuth": true
          }
        }
      ]
    }
  ],
  "globalStyle": {
   "navigationBarTitleText": "smart fisheries",
       "navigationStyle": "custom",
       "navigationBarTextStyle": "black",
       "navigationBarBackgroundColor": "#FFFFFF",
       "backgroundColor": "#e2e2e2"
  },
  "tabBar": {
    "color": "#474747",
    "selectedColor": "#9BC6FC",
    "backgroundColor": "#FFFFFF",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "static/images/tabBar/layout_default_3x.png",
        "selectedIconPath": "static/images/tabBar/layout_selected_3x.png"
      },
      {
        "pagePath": "pages/device/index",
        "text": "站点",
        "iconPath": "static/images/tabBar/assembly_default_3x.png",
        "selectedIconPath": "static/images/tabBar/assembly_selected_3x.png"
      },
      {
        "pagePath": "pages/alarm/index",
        "text": "警告",
        "iconPath": "static/images/tabBar/mod_default_3x.png",
        "selectedIconPath": "static/images/tabBar/mod_selected_3x.png"
      },
      {
        "pagePath": "pages/about/index",
        "text": "我的",
        "iconPath": "static/images/tabBar/my_default_3x.png",
        "selectedIconPath": "static/images/tabBar/my_selected_3x.png"
      }
    ]
  }
}
